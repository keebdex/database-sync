    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Google Docs Updater",
            "program": "${workspaceFolder}/google.js",
            "request": "launch",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "node"
        },
        {
            "name": "Launch Self-Hosted Updater",
            "program": "${workspaceFolder}/self-hosted.js",
            "request": "launch",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "node"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Prune Remote Images",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/jobs/prune-images.js"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Sync Keycap Images",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/jobs/sync-keycap-images.js"
        }
    ]
}